model User {
  id Int @id @default(autoincrement())
  email String @unique
  name String?
  orders Order[]
}

model Order {
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  number Int
  owner User? @relation(fields: [ownerId], references: [id])
  ownerId Int?
}
